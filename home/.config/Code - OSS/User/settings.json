{
    "codestats.apikey": "",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.dragAndDrop": false,
    "editor.find.autoFindInSelection": true,
    "editor.fontFamily": "Hack",
    "editor.fontSize": 12,
    "editor.lineHeight": 19,
    "editor.mouseWheelScrollSensitivity": 1.4,
    "editor.mouseWheelZoom": true,
    "editor.roundedSelection": false,
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.o": true,
        "**/*.a": true,
    },
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.linuxExec": "konsole",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.3,
    "terminal.integrated.scrollback": 50000,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations":
    {
        "statusBar.background": "#333333",
        "statusBar.noFolderBackground": "#333333",
        "statusBar.debuggingBackground": "#665533"
    },
    "rust.mode": "legacy",
    "vsicons.dontShowNewVersionMessage": true,
    "window.newWindowDimensions": "maximized",
    "workbench.iconTheme": "vscode-icons",
    "rust.rustup": {
        "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu",
        "toolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "rust.rls": {
        "useRustfmt": true
    },
    "search.location": "panel",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.pythonPath": "/home/hugo/.pyenv/versions/3.7.1/bin/python",
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "black",
    "workbench.panel.defaultLocation": "right",
    "window.titleBarStyle": "custom",
    "window.menuBarVisibility": "visible",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "eslint.packageManager": "yarn",
    "npm.packageManager": "yarn",
    "tslint.packageManager": "yarn",
}
